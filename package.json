{
  "author": "Refael Ackermann <refael@empeeric.com>",
  "dependencies": {
    "debug": "latest",
    "lodash": "latest",
    "lodash-contrib": "latest",
    "opinion": "latest",
    "socket.io": "latest"
  },
  "description": "load ready socket.io builerplate",
  "devDependencies": {
    "asynctrace": "latest",
    "chai": "latest",
    "grist": "latest",
    "mocha": "latest"
  },
  "enginesStrict": {
    "node": ">= 0.11.13"
  },
  "jshintConfig": {
    "esnext": true,
    "expr": true,
    "forin": true,
    "freeze": true,
    "globals": {
      "after": true,
      "before": true,
      "describe": true,
      "expect": true,
      "it": true
    },
    "immed": true,
    "maxdepth": 4,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "scripturl": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "keywords": [
    "socket.io"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "empeericaload",
  "repository": "Empeeric/empeericaload",
  "scripts": {
    "start": "node --harmony index.js",
    "test": "mocha --harmony -r asynctrace"
  },
  "version": "0.0.0"
}
